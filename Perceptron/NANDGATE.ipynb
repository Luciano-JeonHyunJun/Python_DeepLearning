   "source": [
    "import numpy as np\n",
    "\n",
    "def NAND(x1,x2):\n",
    "    x = np.array([x1 , x2])\n",
    "    w = np.array([0.5,0.5])\n",
    "    b = 0.7\n",
    "    \n",
    "    tmp = np.sum(w*x) + b\n",
    "    if tmp <= 0:\n",
    "        return 0;\n",
    "    else:\n",
    "        return 1\n",
    "print(NAND)"
   ]
  }
